 <category domain="/locations/canadian_rockies">Canadian Rockies</category>
 <category domain="/locations/icefields_parkway">Icefields Parkway</category>
 <category domain="/first_ascents">First Ascent</category>
 <pubDate>Wed, 31 Oct 2007 01:11:03 +0000</pubDate>
 <dc:creator>jon walsh</dc:creator>
 <guid isPermaLink="false">59 at http://jon.dev10.webenabled.net</guid>
