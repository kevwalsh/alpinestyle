mso-font-pitch:variable;
mso-font-signature:3 0 0 0 1 0;}
/* Style Definitions */
p.MsoNormal, li.MsoNormal, div.MsoNormal
{mso-style-parent:&quot;&quot;;
margin-top:0cm;
margin-right:0cm;
margin-bottom:10.0pt;
